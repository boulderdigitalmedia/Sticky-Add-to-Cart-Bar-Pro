datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model StickyEvent {
  id         Int      @id @default(autoincrement())
  shopDomain String
  type       String
  productId  String
  variantId  String
  quantity   Int
  createdAt  DateTime @default(now())
}
